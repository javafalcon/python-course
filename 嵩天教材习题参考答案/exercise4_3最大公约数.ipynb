   "source": [
    "n1, n2 = eval(input('请输入两个整数，中间用逗号隔开：'))\n",
    "x, y = n1, n2\n",
    "r = x % y\n",
    "while r > 0:\n",
    "    x = y\n",
    "    y = r\n",
    "    r = x % y\n",
    "\n",
    "print('{}和{}最大公约数是：{}， 最小公倍数是：{}'.format(n1, n2, y, n1 * n2 / y))"
   ]
  }
