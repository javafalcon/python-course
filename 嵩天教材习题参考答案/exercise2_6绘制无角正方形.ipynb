   "source": [
    "import turtle\n",
    "turtle.setup(400,400,40,40)\n",
    "theta = 270 \n",
    "turtle.penup()\n",
    "for i in range(4):\n",
    "    turtle.seth(theta)\n",
    "    turtle.fd(10)\n",
    "    turtle.pendown()\n",
    "    turtle.fd(40)\n",
    "    turtle.penup()\n",
    "    turtle.fd(10)\n",
    "    theta = theta + 90"
   ]
